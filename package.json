  "main": "dist/globalstorage.js",
  "types": "dist/globalstorage.d.ts",
  "engines": {
    "node": ">=12.16.x",
    "yarn": ">=1.21.x"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
